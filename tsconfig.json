{
  "compilerOptions": {
    "target": "ES2022",
    "module": "CommonJS",
    "declaration": true,
    "outDir": "./bot",
    "strict": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
  },
  "include": [
    "src"
  ],
  "exclude": [
    "node_modules",
    "legacy",
    "**/__tests__/*"
  ],
  "typedocOptions": {
    "entryPoints": ["./src"],
    "entryPointStrategy": "expand",
    "out": "docs"
  }
}